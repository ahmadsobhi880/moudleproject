@model IEnumerable<IdintityProject.Models.CourseModel>

@{
    ViewBag.Title = "Prices";
    Layout = "~/Views/Shared/LayoutLoggedIn.cshtml";
}

<h2>Prices</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CourseDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course_Price)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CourseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course_Price)
        </td>
        
        
    </tr>
}

</table>
