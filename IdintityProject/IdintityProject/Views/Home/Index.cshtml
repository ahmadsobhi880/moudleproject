<body class="main-layout">

    <!-- Page Content  -->

    <div class="p-10 p-md-15 mb-14">

        <!-- business -->
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="titlepage">

                        <h2>Better position of Business</h2>
                        <p>It is a long established fact that a reader will be distracted by the readable content of a page </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-10 offset-md-1">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="business_box ">
                                <figure><img src="~/bootstrap/dashboardPage/images/business_img.jpg" alt="#" /></figure>
                                <p>The College offers undergraduate studies in Architecture, Visual Communication, Computer Science, Software Engineering, Electrical and Electronics Engineering, Chemical Engineering, Building Engineering, Mechanical Engineering and Industrial Engineering and Management; As well as a variety of advanced engineering studies for a master’s degree</p>
                                <a class="read_more" href="#contact1">Request Registering</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- end business -->


        <br />
        <br />
        <div class="contact1" id="contact1">
            <div class="container-contact1">
                <div class="contact1-pic js-tilt" data-tilt>
                    <img src="~/ContactStyles/images/img-01.png" alt="IMG">
                </div>
                @using (Html.BeginForm("Index", "Home", FormMethod.Post, new { @class = "contact1-form validate-form " }))
                {
                    @Html.AntiForgeryToken()

                    <span class="contact1-form-title" style="margin-right:100px;">
                        Get in touch
                    </span>



                    <label>Full Name</label>

                    <div class="wrap-input1 validate-input" data-validate="Name is required">

                        @Html.TextBoxFor(m => m.FullNameRequests, new { @placeholder = "Full Name..", @class = "input1" })

                        <span class="shadow-input1"></span>
                    </div>

                    <label>Email Address</label>

                    <div class="wrap-input1 validate-input" data-validate="Valid email is required: ex@abc.xyz">

                        @Html.TextBoxFor(m => m.EmailRequests, new { @placeholder = "Email..", @class = "input1" })

                        <span class="shadow-input1"></span>
                    </div>

                    <label>Phone number</label>

                    <div class="wrap-input1 validate-input" data-validate="Subject is required">

                        @Html.TextBoxFor(m => m.PhoneNumberRequests, new { @placeholder = "Phone number..", @class = "input1" })

                        <span class="shadow-input1"></span>
                    </div>

                    <br />
                    <div class="container-contact1-form-btn">

                        <input type="submit" value="Send request" class="contact1-form-btn" style="margin-right:100px;">

                    </div>
                }

            </div>
        </div>




        <div class="container ">

            <div class="row">
                <div class="col-md-12 ">
                    <div class="row">

                        <div id="cards" class="col text-center cards ">


                            <div class="row mb-2">
                                <div class="col-md-6">
                                    <div class="row g-0 border rounded overflow-hidden flex-md-row mb-4 shadow-sm h-md-250 position-relative">
                                        <div class="col p-4 d-flex flex-column position-static">
                                            <strong class="d-inline-block mb-2 text-primary">World</strong>
                                            <h3 class="mb-0">Featured post</h3>
                                            <div class="mb-1 text-muted">Nov 12</div>
                                            <p class="card-text mb-auto">
                                                "A challenging learning experience and at a very professional level that contributed to my integration into the industry.

                                                The level of courses and lecturers was very high, which contributed to us integrating into the strong current industry and the tension was high. "
                                            </p>
                                            <a href="#" class="stretched-link">Continue reading</a>
                                        </div>
                                        <div class="col-auto d-none d-lg-block">
                                            <img src="https://media.istockphoto.com/photos/dreamy-arab-guy-freelancer-working-from-home-using-laptop-picture-id1331298245?b=1&k=20&m=1331298245&s=170667a&w=0&h=nJJGzr-iskXDV_AjXhp03BErbtKuSkrelBIaKBgKO1g="
                                                 class="rounded-circle"
                                                 width="300px" />

                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="row g-0 border rounded overflow-hidden flex-md-row mb-4 shadow-sm h-md-250 position-relative">
                                        <div class="col p-4 d-flex flex-column position-static">
                                            <strong class="d-inline-block mb-2 text-success">Design</strong>
                                            <h3 class="mb-0">Post title</h3>
                                            <div class="mb-1 text-muted">Nov 11</div>
                                            <p class="mb-auto">
                                                “I enrolled in the SCE following excellent recommendations and opinions about the college, the level of teaching, a broad academic envelope and the personal attitude towards the students.


                                            </p>
                                            <a href="#" class="stretched-link">Continue reading</a>
                                        </div>
                                        <div class="col-auto d-none d-lg-block ">

                                            <img src="https://www.pngitem.com/pimgs/m/20-207983_student-png-girls-student-png-transparent-png.png"
                                                 class="rounded-circle"
                                                 width="300px" />

                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>




                    </div>

                    <section id="counts" class="counts section-bg">
                        <div class="container">

                            <div class="row counters">

                                <div class="col-lg-3 col-6 text-center">
                                    <span data-purecounter-start="0" data-purecounter-end="1232" data-purecounter-duration="1" class="purecounter"></span>
                                    <p>Students 7658</p>
                                </div>

                                <div class="col-lg-3 col-6 text-center">
                                    <span data-purecounter-start="0" data-purecounter-end="64" data-purecounter-duration="1" class="purecounter"></span>
                                    <p>Courses 221</p>
                                </div>

                                <div class="col-lg-3 col-6 text-center">
                                    <span data-purecounter-start="0" data-purecounter-end="42" data-purecounter-duration="1" class="purecounter"></span>
                                    <p>Events 46</p>
                                </div>

                                <div class="col-lg-3 col-6 text-center">
                                    <span data-purecounter-start="0" data-purecounter-end="15" data-purecounter-duration="1" class="purecounter"></span>
                                    <p>Trainers 679</p>
