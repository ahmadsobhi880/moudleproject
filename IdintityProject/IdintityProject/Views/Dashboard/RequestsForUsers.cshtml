@model IEnumerable<IdintityProject.Models.UserRequestsModel>

@{
    ViewBag.Title = "RequestsForUsers";
    Layout = "~/Views/Shared/LayoutLoggedIn.cshtml";
}

<h3>Registration Requests For Users</h3>
<br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FullNameRequests)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailRequests)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumberRequests)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullNameRequests)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailRequests)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumberRequests)
        </td>
        <td>
           
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
